        "laravel-mix": "^5.0.0",
        "lodash": "^4.17.19",
        "popper.js": "^1.12.9",
        "vue": "^2.5.7"
    },
    "dependencies": {
        "datatables.net": "^1.10.15",
        "datatables.net-bs": "^1.10.15",
        "datatables.net-buttons": "^1.4.0",
        "datatables.net-buttons-bs": "^1.4.0",
        "gulp-sass": "^4.0.2",
        "node-gyp": "^5.0.3",
        "npm": "^6.11.3"
    }
